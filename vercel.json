{
  "version": 2,
  "buildCommand": "cd cyber-buddha-blessing && npm ci && npm run build",
  "outputDirectory": "cyber-buddha-blessing/.next",
  "installCommand": "cd cyber-buddha-blessing && npm ci",
  "builds": [
    {
      "src": "cyber-buddha-blessing/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}