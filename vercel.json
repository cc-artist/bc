{
  "version": 2,
  "installCommand": "cd cyber-buddha-blessing && npm install --legacy-peer-deps --force",
  "buildCommand": "cd cyber-buddha-blessing && npm run build",
  "builds": [
    {
      "src": "cyber-buddha-blessing/package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "NODE_VERSION": "20.x"
  }
}