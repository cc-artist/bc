{
  "version": 2,
  "scope": "cyber-buddha",
  "framework": "nextjs",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/admin/login",
      "dest": "/admin/login",
      "methods": ["GET", "POST"],
      "continue": true
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "continue": true
    }
  ]
}